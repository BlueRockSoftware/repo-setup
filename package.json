{
    "name": "setup-repo",
    "version": "1.0.0",
    "description": "GitHub Action to setup Google Repo",
    "main": "lib/main.js",
    "scripts": {
        "build": "tsc",
        "check": "eslint src/**/*.ts",
        "format": "prettier --write **/*.ts",
        "lint": "eslint src/**/*.ts --quiet --fix",
        "package": "ncc build --source-map --license licenses.txt",
        "all": "npm run build && npm run package"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BlueRockSoftware/setup-repo.git"
    },
    "keywords": [
        "actions",
        "github-actions",
        "setup-repo",
        "repo"
    ],
    "author": "Ben Payne",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/BlueRockSoftware/setup-repo/issues"
    },
    "homepage": "https://github.com/BlueRockSoftware/setup-repo#readme",
    "devDependencies": {
        "@types/node": "^12.0.0",
        "@typescript-eslint/eslint-plugin": "^2.30.0",
        "@typescript-eslint/parser": "^2.30.0",
        "eslint": "^6.8.0",
        "prettier": "^2.2.1",
        "typescript": "^3.8.3",
        "@vercel/ncc": "^0.36.1"
    },
    "dependencies": {
        "@actions/core": "^1.2.4",
        "@actions/exec": "^1.1.1",
        "@actions/tool-cache": "^1.3.4",
        "tcs": "^10.0.2"
    }
}
